-- Booting the Library
local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/jensonhirst/Orion/main/source'))()

-- Create Main Window
local Window = OrionLib:MakeWindow({
    Name = "EXECUTIVERAT IN THE SEWER",
    HidePremium = false,
    SaveConfig = false,
    IntroEnabled = true,
    IntroText = "EXECUTIVERAT",
    ConfigFolder = "OrionConfig"
})

-- Initial Notification
OrionLib:MakeNotification({
    Name = "EXECUTIVERAT",
    Content = "Snatched Scripts - Summer Event Script 9999+ points",
    Time = 5
})

-- Tab 1: Loader
local LoaderTab = Window:MakeTab({
    Name = "Loader",
    Icon = "rbxassetid://7733955741"
})

-- [Previous Loader Tab buttons remain unchanged...]

-- Tab 2: DUPE/VULN (Key System)
local DupeTab = Window:MakeTab({
    Name = "DUPE/VULN",
    Icon = "rbxassetid://7734068321"
})

-- Key System
local correctKey = "EXECUTIVERAT_FREE_aMB0Ts12s4gjaivmr_var111113321321123"
local keyInput = ""
local keyVerified = false

-- Copy Key Link Button
DupeTab:AddButton({
    Name = "Copy Key Link",
    Callback = function()
        setclipboard("https://link-center.net/431614/EQVrnV89bzeb")
        OrionLib:MakeNotification({
            Name = "COPIED",
            Content = "Key link copied to clipboard!",
            Time = 3
        })
    end
})

-- Key Input
DupeTab:AddTextbox({
    Name = "Enter Key",
    Default = "",
    TextDisappear = false,
    Callback = function(Value)
        keyInput = Value
    end
})

-- Submit Key Button
DupeTab:AddButton({
    Name = "Submit Key",
    Callback = function()
        if keyInput == correctKey then
            keyVerified = true
            OrionLib:MakeNotification({
                Name = "SUCCESS",
                Content = "Key accepted! Loading DUPE/VULN features...",
                Time = 3
            })
            LoadDupeFeatures()
        else
            OrionLib:MakeNotification({
                Name = "ERROR",
                Content = "Invalid key!",
                Time = 3
            })
        end
    end
})

-- Function to load DUPE features after key verification
function LoadDupeFeatures()
    -- Clear existing elements
    for _,v in pairs(DupeTab:GetChildren()) do
        if v:IsA("TextLabel") or v:IsA("TextButton") then
            v:Destroy()
        end
    end

    -- VULN Features (PATCHED)
    DupeTab:AddLabel("Skip Craft Time (PATCHED)")
    DupeTab:AddLabel("Skip Seed Time (PATCHED)")
    DupeTab:AddButton({Name = "Egg ESP", Callback = function() end})
    
    -- Egg Finder Dropdown
    DupeTab:AddDropdown({
        Name = "Egg Finder",
        Default = "Select Egg",
        Options = {
            "Raccoon (Night Egg)", 
            "Disco Bee (Anti Bee Egg)", 
            "Butterfly (Anti Bee Egg)", 
            "Queen Bee (Bee Egg)", 
            "Red Fox (Mythical Egg)", 
            "Mimic Octopus (Paradise Egg)"
        },
        Callback = function(Value) end
    })
    
    DupeTab:AddButton({Name = "Egg Hop", Callback = function() end})

    -- DUPE Features
    DupeTab:AddLabel("DUPE -")
    
    -- Pet Selection
    DupeTab:AddDropdown({
        Name = "Select Pet",
        Default = "Select Pet",
        Options = {
            "Raccoon", 
            "Disco Bee", 
            "Butterfly", 
            "Queen Bee", 
            "Red Fox", 
            "Mimic Octopus"
        },
        Callback = function(Value) end
    })
    
    -- Size and Age Inputs
    DupeTab:AddTextbox({
        Name = "Size (Number)",
        Default = "",
        TextDisappear = false,
        Callback = function(Value) end
    })
    
    DupeTab:AddTextbox({
        Name = "Age (Number)",
        Default = "",
        TextDisappear = false,
        Callback = function(Value) end
    })
    
    DupeTab:AddLabel("Server Dupe Compatibility: Not Compatible")
    
    -- Steal Fruits Section
    DupeTab:AddLabel("Steal Others Fruit")
    DupeTab:AddTextbox({
        Name = "Username",
        Default = "",
        TextDisappear = false,
        Callback = function(Value) end
    })
    
    DupeTab:AddButton({
        Name = "Whitelist User",
        Callback = function() end
    })
    
    -- Shovel Section
    DupeTab:AddLabel("Shovel Other Garden Fruits")
    DupeTab:AddTextbox({
        Name = "Username",
        Default = "",
        TextDisappear = false,
        Callback = function(Value) end
    })
    
    DupeTab:AddButton({
        Name = "Whitelist User",
        Callback = function() end
    })
    
    -- Teleport Button
    DupeTab:AddButton({
        Name = "Teleport to EASTER SHOP",
        Callback = function() 
            -- Add teleport script here
        end
    })
    
    -- Status Message
    DupeTab:AddParagraph(
        "STATUS", 
        "ALL VULN SCRIPTS ARE PATCHED AND NOW FIXING, DUPE WILL BE COMING TONIGHT LATER"
    )
end

-- [Rest of your tabs remain unchanged...]

-- Initialize UI
OrionLib:Init()
